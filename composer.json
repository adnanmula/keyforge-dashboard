{
    "name": "adnanmula/card-game-stats",
    "description": "",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "beberlei/assert": "^3.3",
        "doctrine/dbal": "^2.10",
        "ramsey/uuid": "^4.0",
        "robmorgan/phinx": "^0.12.11",
        "symfony/asset": "6.0.*",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "6.0.*",
        "symfony/http-client": "6.0.*",
        "symfony/messenger": "6.0.*",
        "symfony/twig-bundle": "6.0.*",
        "symfony/yaml": "6.0.*"
    },
    "require-dev": {
        "adnanmula/coding-standard": "^0.0.5",
        "behat/behat": "^3.10",
        "friends-of-behat/symfony-extension": "^2.0",
        "phpro/grumphp": "^1.10",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin",
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "AdnanMula\\Cards\\": "src/",
            "AdnanMula\\Cards\\Migrations\\": "migrations/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdnanMula\\Cards\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "rm -rf .git/hooks",
            "cp -r scripts/git-hooks .git/hooks"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
